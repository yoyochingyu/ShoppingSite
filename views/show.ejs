<%-include("partials/header")%>
<div class="container">
    <div class="card mb-3 mt-5 mx-auto">
        <div class="row no-gutters">
          <div class="col-md-5">
            <img src="<%=product.imageUrl%>" class="card-img" alt="https://miro.medium.com/max/880/0*H3jZONKqRuAAeHnG.jpg" >
          </div>
          <div class="col-md-7">
            <div class="card-body">
              <h3 class="card-title text-center"><%=product.name%></h3>
              <p class="card-text text-center"><%=product.description%></p>
            </div>
          </div>
        </div>
      </div>
</div>


<%-include("partials/footer")%>